<!DOCTYPE web-app PUBLIC
  "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
  "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
    <servlet-name>Enter_the_Room</servlet-name>
    <servlet-class>Enter_the_Room</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TEST</servlet-name>
    <servlet-class>Default.TEST</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Enter_the_Room </servlet-name>
    <url-pattern>/servlet/Enter_the_Room </url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TEST</servlet-name>
    <url-pattern>/servlet/TEST</url-pattern>
  </servlet-mapping>
</web-app>
